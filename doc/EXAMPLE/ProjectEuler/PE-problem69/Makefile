######
#
# A simple Makefile
#
######

ATSHOMEQ="$(ATSHOME)"
ATSCC=$(or $(ATSHOMEQ), "/usr")/bin/atscc

######

checkall: compall cleanall
compall:: \
  problem69-hwxi \

######

problem69-hwxi: problem69-hwxi.dats
	$(ATSCC) -O2 -o $@ problem69-hwxi.dats && ./problem69-hwxi

######

clean::
	rm -f *~ *_?ats.c *_?ats.o

cleanall: clean
	rm -f problem69-hwxi

###### end of [Makefile] ######
